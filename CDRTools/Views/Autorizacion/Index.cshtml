@model IEnumerable<CDRTools.Models.Autorizacion>

<p>
    @Html.ActionLink("Create New", "Crear")
</p>
<table class="table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Autorizacion_Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Id_Autorizacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Id_Extension)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Autorizacion_Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id_Autorizacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id_Extension)
        </td>
       
        <td>
            @Html.ActionLink("Editar", "Editar", new { id=item.Id_Autorizacion }) |
           
            @Html.ActionLink("Eliminar", "Eliminar", new { id=item.Id_Autorizacion })
        </td>
    </tr>
}

</table>
