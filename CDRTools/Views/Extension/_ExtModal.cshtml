    <script type="text/javascript">
    $("#sel1").change(function () {
        var x = document.getElementById("sel1").value;
        if (x == 1)
        {
            var fch = new Date();
            var a = fch.getFullYear().toString();
            var m = fch.getMonth();
            var d = fch.getDate();
            var ms,ds;
            if (m < 10)
            {
                m++;
                ms = m.toString();
                if(m<10)
                ms = "0" + ms;
            }
            else
                {
                m++;
                ms = m.toString();
                }

            if (d <= 9)
                ds = "0" + d.toString();
            else
                ds = d.toString();

            valf = a + "-" + ms + "-" + ds;
            document.getElementById("f1").value = valf;
            document.getElementById("f2").value = valf;
            $("#f1").slideDown();
            $("#f2").slideDown();
        }
        else
        {
            $("#f1").slideUp();
            $("#f2").slideUp();
        }
    });
    </script>

    <script type="text/javascript">
        $(function () {
            function moveItems(origin, dest) {
                $(origin).find(':selected').appendTo(dest);
            }

            function moveAllItems(origin, dest) {
                $(origin).children().appendTo(dest);
            }

            $('#left').click(function () {
                moveItems('#sbTwo', '#sbOne');
            });

            $('#right').on('click', function () {
                moveItems('#sbOne', '#sbTwo');
            });

            $('#leftall').on('click', function () {
                moveAllItems('#sbTwo', '#sbOne');
            });

            $('#rightall').on('click', function () {
                moveAllItems('#sbOne', '#sbTwo');
            });
        });
    </script>


    <script>
    (function () {
        var s = 0;
        var salt;
        $('#btnRight').click(function (e) {
            var selectedOpts = $('#lstBox1 option:selected');
            if (selectedOpts.length == 0) {
                alert("Nothing to move.");
                e.preventDefault();
            }
            $('#lstBox2').append($(selectedOpts).clone());
            $(selectedOpts).attr('disabled', 'disabled');
            $(selectedOpts).css('color', '#bfbfc0');
            e.preventDefault();
        });
        $('#btnAllRight').click(function (e) {
            var selectedOpts = $('#lstBox1 option:enabled');
            if (selectedOpts.length == 0) {
                alert("Nothing to move.");
                e.preventDefault();
            }
            $('#lstBox2').append($(selectedOpts).clone());
            $(selectedOpts).attr('disabled', 'disabled');
            $(selectedOpts).css('color', '#bfbfc0');
            e.preventDefault();
        });
        $('#btnLeft').click(function (e) {
            var selectedOpts = $('#lstBox2 option:selected');
            if (selectedOpts.length == 0) {
                alert("Nothing to move.");
                e.preventDefault();
            }
            $(selectedOpts).each(function () {
                s=$(this).val();
                $("#lstBox1 option[value="+s+"]").removeAttr('disabled');
                $("#lstBox1 option[value=" + s + "]").css('color', '#6f6f6f');
            });
            $(selectedOpts).remove();
            e.preventDefault();
        });
        $('#btnAllLeft').click(function (e) {
            var selectedOpts = $('#lstBox2 option');
            if (selectedOpts.length == 0) {
                alert("Nothing to move.");
                e.preventDefault();
            }
            $(selectedOpts).each(function () {
                s = $(this).val();
                $("#lstBox1 option[value=" + s + "]").removeAttr('disabled');
                $("#lstBox1 option[value=" + s + "]").css('color', '#6f6f6f');
            });
            $(selectedOpts).remove();
            e.preventDefault();
        });
    }(jQuery));
    </script>

