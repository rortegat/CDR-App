<script type="text/javascript">

    $('#send').click(function (event) {
        slc = $('#sbTwo option:selected').val();
        s = slc.substring(0,4); 
        op = $('#sel1 option:selected').val();
        var fch = new Date();
        var a = fch.getFullYear().toString();
        var m = fch.getMonth();
        var d = fch.getDate();
        var dy = fch.getDay();
        var ms, ds;
        var hi = " 00:00:00.000"
        var hf = " 23:59:59.999"

        if (m < 10) {
            m++;
            ms = m.toString();
            if (m < 10)
                ms = "0" + ms;
        }
        else {
            m++;
            ms = m.toString();
        }

        if (d <= 9)
            ds = "0" + d.toString();
        else
            ds = d.toString();

        switch (op) {
            case "1":
                //code block
                break;
            case "2":
                //Hoy
                fi = (a  + ms  + ds).shortDateFormat();
                ff = a + "-" + ms + "-" + ds + hf;
                break;
            case "3":
                //Ayer
                fi = a + "-" + ms + "-" + (ds-1) + hi;
                ff = a + "-" + ms + "-" + (ds-1) + hf;
                break;
            case "4":
                //Esta semana
                fi = a + "-" + ms + "-" + (ds-dy) + hi;
                ff = a + "-" + ms + "-" + (parseInt(ds)+(6-dy)) + hf;
                break;
            case "5":
                //Este mes
                fi = a + "-" + ms + "-" + ("0" + (ds - (ds - 1)).toString()) + hi;
                if (ms == 0 || ms == 2 || ms == 4 || ms == 6 || ms == 7 || ms == 9 || ms == 11)
                    dsm=31;
                if(ms == 3 || ms == 5 || ms == 8 || ms == 10)
                    dsm = 31;
                if(ms == 1)
                    dsm = 28;
                ff = a + "-" + ms + "-" + dsm + hf;
                break;
            case "6":
                //Mes pasado
                ms--;
                fi = a + "-" + ("0" + ms) + "-" + ("0" + (ds - (ds - 1)).toString()) + hi;
                if (ms == 0 || ms == 2 || ms == 4 || ms == 6 || ms == 7 || ms == 9 || ms == 11)
                    dsm = 31;
                if (ms == 3 || ms == 5 || ms == 8 || ms == 10)
                    dsm = 31;
                if (ms == 1)
                    dsm = 28;
                ff = a + "-" + ("0" + ms) + "-" + dsm + hf;
                break;
            case "7":
                //Este año
                fi = a + "-01-01" + hi;
                ff = a + "-12-31" + hf;
                break;
            case "8":
                //Año pasado
                fi = 2016 + "-01-01" + hi;
                ff = 2016 + "-12-31" + hf;
                break;
            default:
                alert("Default!!!");
                //code block
        }

        $.post("/Reporte/_ShowData", { ini: fi, fin: ff, ex: s }, function (data) {
            //$(gridContent).hide();
            $("#gridContent").html(data);
        }).fail(function (response) {
            alert("Error");
        });
    })
</script>